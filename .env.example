# TT FIX Order Adapter - Environment Variables
# Copy this file to .env and update with your actual values

# FIX Connection Settings
SENDER_COMP_ID=YOUR_ACTUAL_SENDER_COMP_ID
TARGET_COMP_ID=TTUAT_SERVER
FIX_PASSWORD=your_fix_password_if_required

# ZMQ Settings
ZMQ_ORDER_PORT=5555
ZMQ_BIND_ADDRESS=tcp://*:5555

# Trading Settings
DEFAULT_ACCOUNT=YOUR_DEFAULT_ACCOUNT
RISK_MANAGEMENT_ENABLED=true
MAX_ORDER_SIZE=1000
MAX_DAILY_VOLUME=10000

# Logging Settings
LOG_LEVEL=INFO
LOG_DIR=data/logs

# Monitoring Settings
MONITORING_ENABLED=true
METRICS_PORT=8080
HEALTH_CHECK_INTERVAL=30

# SSL Certificate Paths
SSL_CERT_PATH=./TTFIX.crt
CLIENT_CERT_PATH=./client.crt
PRIVATE_KEY_PATH=./private.key

# Database Settings (if using external database)
# DATABASE_URL=postgresql://user:pass@localhost/ttfix
# REDIS_URL=redis://localhost:6379/0
